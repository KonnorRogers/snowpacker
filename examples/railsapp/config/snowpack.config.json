      {
        "installOptions": {
          "NODE_ENV": true
        },
        "scripts": {
          "mount:javascript": "mount app/javascript/snowpacks --to /javascript",
          "mount:stylesheets": "mount app/javascript/stylesheets --to /stylesheets",
          "mount:assets": "mount app/javascript/assets --to /assets",
          "mount:snowpacks": "mount snowpacks --to /"
        },
        "devOptions": {
          "bundle": false,
          "port": 4035,
          "open": "none",
          "out": "snowpacks"
        },
        "proxy": {
          "0.0.0.0:4035": "0.0.0.0:4035"
        },
        "buildOptions": {
          "clean": true,
          "baseUrl": "/"
        },
        "plugins": [
          "@snowpack/plugin-babel"
        ]
      }
